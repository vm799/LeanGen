# Server
NODE_ENV=development
PORT=3001

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/leadgenius
REDIS_URL=redis://localhost:6379

# Google APIs
GOOGLE_MAPS_API_KEY=your_maps_api_key_here
GOOGLE_PLACES_API_KEY=your_places_api_key_here
GOOGLE_SEARCH_API_KEY=your_search_api_key_here
GOOGLE_SEARCH_CX=your_search_engine_id_here

# AI
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-exp

# Rate Limiting
DAILY_PLACES_SEARCH_LIMIT=500
DAILY_PLACES_DETAILS_LIMIT=1000
DAILY_GEMINI_LIMIT=2000
DAILY_SEARCH_LIMIT=200

# Monitoring (optional)
SENTRY_DSN=your_sentry_dsn_here

# CORS
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Authentication (Clerk)
CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...

# Payments (Stripe)
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
